    "tslint-react": "^3.6.0",
    "typescript": "^2.9.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "tslint --fix",
      "git add"
    ],
    "*.{js,css,md}": [
      "prettier --write",
      "git add"
    ]
  }
